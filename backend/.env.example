# ── Ollama ────────────────────────────────────────
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=qwen2.5-coder:3b
OLLAMA_TIMEOUT_SECONDS=120

# ── OpenMetadata ──────────────────────────────────
OPENMETADATA_HOST=http://localhost:8585
# Get this from: OM UI → Settings → Bots → ingestion-bot
OPENMETADATA_JWT_TOKEN=

# ── FastAPI ───────────────────────────────────────
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:5173

# ── Profiling ─────────────────────────────────────
# Max rows before profiling is skipped (safety limit)
MAX_PROFILE_ROWS=10000000
# Comma-separated column name hints for freshness detection
FRESHNESS_COLUMNS=updated_at,modified_at,created_at,timestamp,date_modified,last_updated

# ── Logging ───────────────────────────────────────
LOG_LEVEL=INFO
